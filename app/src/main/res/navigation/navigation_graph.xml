<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph"
    app:startDestination="@id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.kerkar.home.Home_fragment"
        android:label="@string/home"
        tools:layout="@layout/activity_home">
        <action
            android:id="@+id/action_nav_home_to_nav_timetable"
            app:destination="@id/nav_timetable" />
    </fragment>
    <fragment
        android:id="@+id/nav_assignment_list"
        android:name="com.example.kerkar.assignment_list.Assignment_list_fragment"
        android:label="@string/assignment"
        tools:layout="@layout/activity_assignment_list"/>
    <fragment
        android:id="@+id/nav_timetable"
        android:name="com.example.kerkar.Timetable_fragment"
        android:label="@string/timetable"
        tools:layout="@layout/activity_timetable"/>
    <fragment
        android:id="@+id/nav_setting"
        android:name="com.example.kerkar.Setting_fragment"
        android:label="@string/setting"
        tools:layout="@layout/activity_setting"/>
</navigation>